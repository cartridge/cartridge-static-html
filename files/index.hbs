{{> header }}

<!-- Welcome page code START -->
<style>
	* {
		box-sizing: border-box;
	}

	body {
		background-color: #EFEFEF;
		font-family: sans-serif;
	}

	body:before {
		background-image: url(data:image/svg+xml;base64,);
		background-repeat: no-repeat;
		background-position: top center;
		content: '';
		display: block;
		height: 100px;
		margin: 40px;
	}

	h1,
	h2 {
		margin-bottom: 0;
	}

	h1 {
		margin-top: 0;
	}

	h2 {
		padding-top: 8px;
		border-top: 1px solid #B3B3B3;
	}

	.container {
		max-width: 680px;
		margin: 0 auto;
		padding-top: 100px;
		padding: 24px;
		background: #FFF;
		box-shadow: 0 0.5px 0 0 #FFF inset, 0 1px 2px 0 #B3B3B3;
		line-height: 1.6;
	}

	code {
		padding: 4px;
		background: #EDEDED;
	}

	ul {
		margin-bottom: 0;
		list-style: none;
		padding-left: 16px;
	}

	li+li {
		margin-top: 4px;
	}
</style>

<div class="container">
	<h1>You're all up and running!</h1>
	<p>If you're reading this, <code>cartridge-static-html</code> is working!</p>
	<h2>Setup checklist</h2>
	<ul>
		<li>
			<input type="checkbox" id="line-1" />
			<label for="line-1">Add any <code>link</code> stylesheets you may need</label>
		</li>
		<li>
			<input type="checkbox" id="line-2" />
			<label for="line-2">Add any JavaScript files you may need</label>
		</li>
		<li>
			<input type="checkbox" id="line-3" />
			<label for="line-3">Update <code>title</code></label>
		</li>
		<li>
			<input type="checkbox" id="line-4" />
			<label for="line-4">Update/Remove favicon image details</label>
		</li>
		<li>
			<input type="checkbox" id="line-5" />
			<label for="line-5">Update/Remove Facebook meta information</label>
		</li>
		<li>
			<input type="checkbox" id="line-6" />
			<label for="line-6">Update/Remove Twitter meta information</label>
		</li>
		<li>
			<input type="checkbox" id="line-7" />
			<label for="line-7">Update/Remove Google Plus meta information</label>
		</li>
	</ul>
</div>
